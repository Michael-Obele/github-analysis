{
	"name": "github-commit-analyzer",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@sveltejs/adapter-netlify": "^1.0.0-next.67",
		"@sveltejs/adapter-vercel": "^1.0.0-next.60",
		"@sveltejs/kit": "^1.0.0-next.368",
		"@types/cookies": "^0.7.7",
		"autoprefixer": "^10.4.7",
		"cookie": "^0.5.0",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.49.0",
		"svelte-check": "^2.8.0",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.1.5",
		"tailwindcss-elevation": "^1.0.1",
		"tslib": "^2.4.0",
		"typescript": "^4.7.4",
		"vite": "^2.9.14"
	},
	"type": "module",
	"dependencies": {
		"@mdi/js": "^6.9.96",
		"date-fns": "^2.28.0",
		"graphql-tag": "^2.12.6",
		"svelte-ux": "^0.6.10"
	}
}
